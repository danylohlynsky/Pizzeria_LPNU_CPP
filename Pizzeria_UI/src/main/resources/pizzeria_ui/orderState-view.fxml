<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/11.0.14-internal"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="pizzeria_ui.Configuration">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
    <SplitPane dividerPositions="0.66">
        <items>
            <AnchorPane minHeight="0.0" minWidth="0.0">
                <children>
                    <GridPane fx:id="queueGrid" alignment="CENTER" gridLinesVisible="true" prefHeight="1000.0"
                              prefWidth="400.0">
                        <rowConstraints>
                            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                        </rowConstraints>
                        <columnConstraints>
                            <ColumnConstraints minWidth="10.0" prefWidth="100.0"/>
                            <ColumnConstraints minWidth="10.0" prefWidth="100.0"/>
                            <ColumnConstraints minWidth="10.0" prefWidth="100.0"/>
                            <ColumnConstraints minWidth="10.0" prefWidth="100.0"/>
                        </columnConstraints>
                    </GridPane>
                </children>
            </AnchorPane>
            <AnchorPane minHeight="0.0" minWidth="0.0">
                <children>
                    <SplitPane dividerPositions="0.5" orientation="VERTICAL">
                        <items>
                            <AnchorPane minHeight="0.0" minWidth="0.0">
                                <children>
                                    <GridPane>
                                        <columnConstraints>
                                            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                                            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                                        </columnConstraints>
                                        <rowConstraints>
                                            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                        </rowConstraints>
                                        <children>
                                            <Label text="minSecondsForPizza"/>
                                            <Label text="differentPizzaAmount" GridPane.rowIndex="1"/>
                                            <Spinner id="minSecondsForPizza" fx:id="minSecondsForPizza"
                                                     accessibleRole="PAGINATION" GridPane.columnIndex="1"/>
                                            <Spinner fx:id="differentPizzaAmount" GridPane.columnIndex="1"
                                                     GridPane.rowIndex="1"/>
                                            <Label text="tablesAmount" GridPane.rowIndex="2"/>
                                            <Spinner fx:id="tablesAmount" GridPane.columnIndex="1"
                                                     GridPane.rowIndex="2"/>
                                            <Label text="cashiersAmount" GridPane.rowIndex="3"/>
                                            <Label text="cookMode" GridPane.rowIndex="4"/>
                                            <Label text="cooksAmount" GridPane.rowIndex="5"/>
                                            <Spinner fx:id="cashiersAmount" GridPane.columnIndex="1"
                                                     GridPane.rowIndex="3"/>
                                            <Spinner fx:id="cooksAmount" GridPane.columnIndex="1"
                                                     GridPane.rowIndex="5"/>
                                            <ChoiceBox fx:id="cookMode" prefWidth="150.0" GridPane.columnIndex="1"
                                                       GridPane.rowIndex="4"/>
                                            <Button fx:id="Start" alignment="TOP_CENTER" mnemonicParsing="false"
                                                    onMouseReleased="#onStartClick" prefHeight="26.0" prefWidth="110.0"
                                                    text="Start" GridPane.columnSpan="2" GridPane.halignment="CENTER"
                                                    GridPane.rowIndex="6"/>
                                        </children>
                                    </GridPane>
                                </children>
                            </AnchorPane>
                            <AnchorPane minHeight="0.0" minWidth="0.0">
                                <children>
                                    <TableView fx:id="table" prefHeight="200.0" prefWidth="200.0">
                                    </TableView>
                                </children>
                            </AnchorPane>
                        </items>
                    </SplitPane>
                </children>
            </AnchorPane>
        </items>
    </SplitPane>
</VBox>
